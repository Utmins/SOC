{
  "WORD": "\\b\\w+\\b",
  "DATA": ".*?",
  "GREEDYDATA": ".*",
  "BASE10NUM": "(?:[+-]?(?:[0-9]+)(?:\\.[0-9]+)?)",
  "NUMBER": "(?:%{BASE10NUM})",
  "INT": "(?:[+-]?(?:[0-9]+))",
  "POSINT": "\\b(?:[1-9][0-9]*)\\b",

  "MONTH": "\\b(?:Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)\\b",
  "MONTHDAY": "(?:0?[1-9]|[12][0-9]|3[01])",
  "HOUR": "(?:2[0-3]|[01]?[0-9])",
  "MINUTE": "(?:[0-5]?[0-9])",
  "SECOND": "(?:(?:[0-5]?[0-9]|60)(?:[:.,][0-9]+)?)",
  "TIME": "%{HOUR}:%{MINUTE}(?::%{SECOND})?",

  "HOSTNAME": "\\b(?:[A-Za-z0-9][A-Za-z0-9-]{0,62})(?:\\.(?:[A-Za-z0-9][A-Za-z0-9-]{0,62}))*\\b",
  "IP": "(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)(?:\\.(?!$)|$)){4}",
  "IPORHOST": "(?:%{IP}|%{HOSTNAME})",
  "SYSLOGHOST": "%{IPORHOST}",

  "PROG": "(?:[\\w._/%-]+)",
  "SYSLOGPROG": "%{PROG:program}(?:\\[%{POSINT:pid}\\])?",

  "SYSLOGTIMESTAMP": "%{MONTH:month}\\s+%{MONTHDAY:day}\\s+%{TIME:time}",
  "SYSLOGLINE": "%{SYSLOGTIMESTAMP:ts}\\s+%{SYSLOGHOST:host}\\s+%{SYSLOGPROG}:\\s+%{GREEDYDATA:msg}"
}
